<nodeDefs>
    <nodeDef id="controller" nls="ctl">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
        </sts>
        <cmds>
            <sends>
              <cmd id="DON" />
              <cmd id="DOF" />
            </sends>
            <accepts>
              <cmd id="QUERY" />
              <cmd id="DISCOVER" />
              <cmd id="INSTALL_PROFILE" />
            </accepts>
        </cmds>
    </nodeDef>
    <nodeDef id="SPA" nls="MGR">
        <editors />
        <sts>
            <st id="ST-CLITEMP" editor="temp" />
            <st id="ST-GV1" editor="speed" />
            <st id="ST-GV2" editor="speed" />
            <st id="ST-GV3" editor="OnOff" />
            <st id="ST-GV4" editor="OnOff" />
            <st id="ST-GV5" editor="OnOff" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="CMD-MGR-SET_TEMP-NAME">
                  <p id="" editor="temp"  init="ST-CLITEMP" />
                </cmd>
                <cmd id="CMD-MGR-SET_SPEED_P1-NAME">
                   <p id="" editor="speed" init="ST-GV1" />
                </cmd>
                <cmd id="CMD-MGR-SET_SPEED_P2-NAME">
                   <p id="" editor="speed" init="ST-GV2" />
                </cmd>
                <cmd id="CMD-MGR-CIRP-NAME" />
                <cmd id="CMD-MGR-BLOWER-NAME" />
                <cmd id="CMD-MGR-LIGHT-NAME" />
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>
