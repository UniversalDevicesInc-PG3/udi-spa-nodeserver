<nodeDefs>
    <nodeDef id="controller" nls="ctl">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
        </sts>
        <cmds>
            <sends>
              <cmd id="DON" />
              <cmd id="DOF" />
            </sends>
            <accepts>
              <cmd id="QUERY" />
              <cmd id="DISCOVER" />
              <cmd id="INSTALL_PROFILE" />
            </accepts>
        </cmds>
    </nodeDef>
    <nodeDef id="spa" nls="MGR">
        <editors />
        <sts>
            <st id="CLITEMP" editor="temp" />
            <st id="GV1" editor="speed" />
            <st id="GV2" editor="speed" />
            <st id="GV3" editor="OnOff" />
            <st id="GV4" editor="OnOff" />
            <st id="GV5" editor="OnOff" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="SET_TEMP">
                  <p id="" editor="temp"  init="CLITEMP" />
                </cmd>
                <cmd id="SET_SPEED_P1">
                   <p id="" editor="speed" init="GV1" />
                </cmd>
                <cmd id="SET_SPEED_P2">
                   <p id="" editor="speed" init="GV2" />
                </cmd>
                <cmd id="SET_CIRP">
                   <p id="" editor="OnOff" init="GV3" />
                </cmd>
                <cmd id="SET_BLOWER">
                   <p id="" editor="OnOff" init="GV4" /3
                </cmd>
                <cmd id="SET_LIGHT">
                   <p id="" editor="OnOff" init="GV5" />
                </cmd>
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>
